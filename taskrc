# Here is an example of entries that use the default, override and blank values
#   variable=foo   -- By specifying a value, this overrides the default
#   variable=      -- By specifying no value, this means no default
#   #variable=foo  -- By commenting out the line, or deleting it, this uses the default

# Use the command 'task show' to see all defaults and overrides

# Files
data.location=~/Dropbox/Taskwarrior

# Include US holidays
include /usr/local/share/doc/task/rc/holidays.en-US.rc

dateformat.report=a m/d/Y

# Always list all projects (including those with no pending tasks)
list.all.projects=on

# Autocomplete all projects and tags
complete.all.projects=yes
complete.all.tags=yes

default.project=personal
default.priority=M

# Urgency coefficients
urgency.project.coefficient=0
urgency.tags.coefficient=0
urgency.user.tag.deliverable.coefficient=5.0
urgency.user.tag.preparedness.coefficient=4.0

# Reports overrides
report.next.columns=id,project,due.countdown,urgency,description,tags
report.next.labels=ID,Project,Due,Urg,Description,Tags

# Remotes (see 'man task-sync')
merge.default.uri=ftp://'task@noahfrederick.com'@noahfrederick.com/
push.default.uri=ftp://'task@noahfrederick.com'@noahfrederick.com/

# Color customizations
color.due=bold
color.due.today=bright yellow
color.tagged=

# Color theme (uncomment one to use)
#include /usr/local/Cellar/task/2.1.2/share/doc/task/rc/light-16.theme
#include /usr/local/Cellar/task/2.1.2/share/doc/task/rc/light-256.theme
